{
    "name": "q_a_prototype",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "tsc -b && vite build",
        "build:dev": "tsc && vite build --mode dev",
        "lint": "eslint ./src/ui ./tests/ui && prettier --check ./src/ui ./tests/ui",
        "lint:fix": "eslint --fix ./src/ui ./tests/ui  && prettier --write ./src/ui ./tests/ui",
        "preview": "vite preview",
        "test": "vitest --run",
        "test:cov": "vitest --run --coverage",
        "test:watch": "vitest",
        "postinstall": "patch-package"
    },
    "dependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/node": "^22.18.1",
        "@types/react": "^19.1.10",
        "@types/react-dom": "^19.1.7",
        "@vitejs/plugin-react": "^4.7.0",
        "@vitest/coverage-v8": "^3.2.4",
        "eslint": "^9.33.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.3.0",
        "markdownlint-cli": "^0.47.0",
        "patch-package": "^8.0.1",
        "prettier": "^3.6.2",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.39.1",
        "vite": "^4.5.14",
        "vitest": "^3.2.4"
    }
